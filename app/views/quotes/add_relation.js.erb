var id = <%= @quote[:id] %>;
$("#updt_"+id).val("Updated!");
$("#notice_"+id).append("<small>Quote moved to:</small>");
if (window.location.pathname.includes("user")) {
  $("#quote_"+id).fadeOut(500);
  <% if not @list.nil?  %>
    setTimeout(function() {
      $(".tab-pane[id*='<%= @list[:id] %>']" ).append($("#quote_"+id));
    }, 500);
    $("#quote_"+id).fadeIn();
  <% else %>
    setTimeout(function() {
      $(".tab-pane[id*='all']" ).append($("#quote_"+id));
    }, 500);
    $("#quote_"+id).fadeIn();
  <% end %>
}
